    "print(\"The volatility of 3 shares and 7 bonds between {} and {} is\".format(day_1,day_2),\"%.2f\"% (vol_4*100),\"%\")\n",
    "print(\"The volatility of 2 shares and 8 bonds between {} and {} is\".format(day_1,day_2),\"%.2f\"% (vol_5*100),\"%\")"
   ]
